<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated grafcet -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="grafcet.presentation.GrafcetModelWizardID"
            name="%_UI_GrafcetModelWizard_label"
            class="grafcet.presentation.GrafcetModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/GrafcetModelFile.gif">
         <description>%_UI_GrafcetModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated grafcet -->
      <editor
            id="grafcet.presentation.GrafcetEditorID"
            name="%_UI_GrafcetEditor_label"
            icon="icons/full/obj16/GrafcetModelFile.gif"
            extensions="grafcet"
            class="grafcet.presentation.GrafcetEditor"
            contributorClass="grafcet.presentation.GrafcetActionBarContributor">
      </editor>
   </extension>

</plugin>
